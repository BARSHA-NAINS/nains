            <span class="code-keyword">&lt;</span><span class="code-leadattribute">div</span><span class="code-attribute"> </span><span class="code-attribute">class</span><span class="code-keyword">="</span><span class="code-keyword">content"</span><span class="code-keyword">&gt;</span>
                <span class="code-keyword">&lt;</span><span class="code-leadattribute">section</span><span class="code-attribute"> </span><span class="code-attribute">class</span><span class="code-keyword">="</span><span class="code-keyword">main_articles"</span><span class="code-keyword">&gt;</span>
